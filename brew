/bin/bash -c "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install.sh)"

brew update

brew upgrade

brew doctor

echo "Installing all programs I just remembered to install..."

brew install python
brew install tree
brew install vim
brew install neovim
brew install zsh
brew install oh-my-zsh

brew cask install visual-studio-code
